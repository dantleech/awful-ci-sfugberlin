{
    "name": "dantleech/pocket-ci",
    "description": "Pocket CI",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "amphp/amp": "^2.1",
        "amphp/process": "^1.1",
        "phpactor/test-utils": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.4",
        "phpunit/phpunit": "^8.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\PocketCi\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\PocketCi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
